{
	"name": "kasouka",
	"type": "module",
	"private": true,
	"zshy": {
		"exports": {
			".": "./src/index.ts",
			"./datasource/index": "./src/datasource/index.ts",
			"./datasource/csv": "./src/datasource/csv.ts",
			"./datasource/file-table": "./src/datasource/file-table.ts",
			"./datasource/everyuuid": "./src/datasource/everyuuid.ts",
			"./datasource/virtual": "./src/datasource/virtual.ts"
		}
	},
	"scripts": {
		"build": "zshy",
		"prepublishOnly": "bun run build",
		"watch": "zshy && tsc --watch"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"typescript": "^5",
		"zshy": "^0.3.5"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"files": [
		"**/*.js",
		"**/*.mjs",
		"**/*.cjs",
		"**/*.d.ts",
		"**/*.d.mts",
		"**/*.d.cts"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./datasource/index": {
			"types": "./dist/datasource/index.d.cts",
			"import": "./dist/datasource/index.js",
			"require": "./dist/datasource/index.cjs"
		},
		"./datasource/csv": {
			"types": "./dist/datasource/csv.d.cts",
			"import": "./dist/datasource/csv.js",
			"require": "./dist/datasource/csv.cjs"
		},
		"./datasource/file-table": {
			"types": "./dist/datasource/file-table.d.cts",
			"import": "./dist/datasource/file-table.js",
			"require": "./dist/datasource/file-table.cjs"
		},
		"./datasource/everyuuid": {
			"types": "./dist/datasource/everyuuid.d.cts",
			"import": "./dist/datasource/everyuuid.js",
			"require": "./dist/datasource/everyuuid.cjs"
		},
		"./datasource/virtual": {
			"types": "./dist/datasource/virtual.d.cts",
			"import": "./dist/datasource/virtual.js",
			"require": "./dist/datasource/virtual.cjs"
		}
	}
}